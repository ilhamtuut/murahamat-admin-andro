<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="wad.wan.murahamatdistro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".PromoActivity" />
        <activity android:name=".InputBarangActivity" />
        <activity android:name=".TestimonialActivity" />
        <activity android:name=".UserManagementActivity" />
        <activity android:name=".HistoryTransaksiActivity" />
        <activity android:name=".KategoriActivity"></activity>
    </application>

</manifest>